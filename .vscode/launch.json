    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Fortran",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/main",
            
            "stopAtEntry": false,
            
            //"cwd": "${workspaceFolder}/calc_stosszahl/k_eq_1/lower_ball",
            "cwd": "${workspaceFolder}/",
            //"cwd": "${workspaceFolder}/calc_stosszahl/k_eq_05",
            //"cwd": "${workspaceFolder}",
            
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            //"preLaunchTask":"${defaultBuildTask}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
